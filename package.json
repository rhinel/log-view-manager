{
  "name": "log-view-manager",
  "version": "1.2.3",
  "author": "rhinel <rhinelkc@hotmail.com>",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "autoprefixer": "9.6.0",
    "babel-eslint": "10.0.2",
    "babel-jest": "24.8.0",
    "babel-loader": "^8.0.6",
    "babel-preset-react-app": "^9.0.0",
    "babel-runtime": "6.26.0",
    "body-parser": "^1.19.0",
    "case-sensitive-paths-webpack-plugin": "2.2.0",
    "chalk": "2.4.2",
    "compression": "^1.7.4",
    "compression-webpack-plugin": "^3.0.0",
    "css-loader": "3.0.0",
    "dotenv": "8.0.0",
    "dotenv-expand": "5.1.0",
    "element-react": "^1.4.34",
    "element-theme-default": "^1.4.13",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "2.1.2",
    "eslint-plugin-flowtype": "3.11.1",
    "eslint-plugin-import": "2.17.3",
    "eslint-plugin-jsx-a11y": "6.2.1",
    "eslint-plugin-react": "7.14.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "4.0.0",
    "fs-extra": "8.0.1",
    "html-webpack-plugin": "^4.0.0-beta.1",
    "jest": "24.8.0",
    "log4js": "^4.3.2",
    "md5": "^2.2.1",
    "mongoose": "^5.7.5",
    "node-sass": "^4.12.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "promise": "8.0.3",
    "query-string": "6.8.1",
    "raf": "3.4.1",
    "react": "^16.8.6",
    "react-dev-utils": "^9.0.1",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.11.1",
    "react-router-dom": "^5.0.1",
    "redis": "^2.8.0",
    "resolve": "1.11.1",
    "sass-loader": "^7.1.0",
    "style-loader": "0.23.1",
    "superagent": "^5.1.0",
    "supervisor": "^0.12.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "terser-webpack-plugin": "^1.3.0",
    "url-loader": "2.0.0",
    "webpack": "4.35.0",
    "webpack-dev-server": "3.7.2",
    "webpack-manifest-plugin": "2.0.4",
    "whatwg-fetch": "3.0.0"
  },
  "scripts": {
    "git-init": "git submodule update --init --recursive",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "serve": "cd ./server && supervisor --inspect index.js",
    "lint": "eslint --ext .js src server"
  },
  "proxy": "http://localhost",
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "server/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "react-app"
    ],
    "rules": {
      "prefer-destructuring": [
        "error",
        {
          "object": true,
          "array": false
        }
      ],
      "object-curly-newline": [
        "error",
        {
          "consistent": true
        }
      ],
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "jsx-a11y/href-no-hash": 0,
      "class-methods-use-this": 0,
      "no-multi-spaces": 0,
      "linebreak-style": 0,
      "semi": 0,
      "no-underscore-dangle": 0,
      "arrow-parens": 0,
      "no-console": 0
    }
  }
}

{
  "name": "log-view-manager",
  "version": "1.1.18",
  "author": "rhinel <rhinelkc@hotmail.com>",
  "private": true,
  "dependencies": {
    "autoprefixer": "9.1.5",
    "babel-core": "6.26.3",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.4.2",
    "babel-loader": "7.1.5",
    "babel-preset-react-app": "^3.1.2",
    "babel-runtime": "6.26.0",
    "body-parser": "^1.18.3",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "compression": "^1.7.3",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "1.0.0",
    "dotenv": "6.0.0",
    "dotenv-expand": "4.2.0",
    "element-react": "^1.4.21",
    "element-theme-default": "^1.4.13",
    "eslint": "5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-loader": "2.1.0",
    "eslint-plugin-flowtype": "2.50.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.11.1",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "23.5.0",
    "log4js": "^3.0.5",
    "md5": "^2.2.1",
    "mongoose": "^5.2.13",
    "node-sass": "^4.9.3",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "promise": "8.0.1",
    "query-string": "6.1.0",
    "raf": "3.4.0",
    "react": "^16.5.0",
    "react-dev-utils": "^6.0.0-next.a671462c",
    "react-dom": "^16.5.0",
    "react-router-dom": "^4.3.1",
    "redis": "^2.8.0",
    "resolve": "1.8.1",
    "sass-loader": "^7.1.0",
    "style-loader": "0.23.0",
    "superagent": "^3.8.3",
    "supervisor": "^0.12.0",
    "sw-precache-webpack-plugin": "0.11.5",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "1.1.1",
    "webpack": "4.17.2",
    "webpack-dev-server": "3.1.8",
    "webpack-manifest-plugin": "2.0.3",
    "whatwg-fetch": "2.0.4"
  },
  "scripts": {
    "git-init": "git submodule update --init --recursive",
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "serve": "cd ./server && supervisor index.js",
    "lint": "eslint --ext .js src server"
  },
  "proxy": {
    "^/api": {
      "target": "http://localhost",
      "changeOrigin": true
    }
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}",
      "server/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node",
      "mjs"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "eslintConfig": {
    "extends": [
      "airbnb-base",
      "react-app"
    ],
    "rules": {
      "prefer-destructuring": [
        "error",
        {
          "object": true,
          "array": false
        }
      ],
      "object-curly-newline": [
        "error",
        {
          "consistent": true
        }
      ],
      "no-param-reassign": [
        "error",
        {
          "props": false
        }
      ],
      "class-methods-use-this": 0,
      "no-multi-spaces": 0,
      "linebreak-style": 0,
      "semi": 0,
      "no-underscore-dangle": 0,
      "arrow-parens": 0,
      "no-console": 0
    }
  }
}
